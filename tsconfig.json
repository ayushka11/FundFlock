        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "baseUrl": "src",
        "outDir": "dist",
        "sourceMap": true,
        "noImplicitAny": true,
        "types": ["node", "express"],
        "typeRoots": ["node_modules/@types", "src/@types"],
    },
    "include": [
        "src/**/*"
    ],
}
